\n      const crc16Code = crc.crc16xmodem(Buffer.from(payload, 'utf-8')).toString(16).toUpperCase().padStart(4, '0');\n\n      const payloadcompleta = `${payload}${crc16Code}`;\n    }\n  }\n};\n\n</script>\n"],"mappings":"AAmCA,OAAOA,GAAE,MAAO,KAAK;AACrB,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,eAAe,EAAE,EAAE;MACnBC,aAAa,EAAE,QAAQ;MACvBC,iBAAiB,EAAE,UAAU;MAC7BC,mBAAmB,EAAE,SAAS;MAC9BC,WAAW,EAAE,QAAQ;MACrBC,KAAK,EAAE,MAAM;MACbC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,MAAMC,UAAS,GAAIC,UAAU,CAAC,IAAI,CAACb,KAAK,CAACc,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC,CAAC,CAACC,OAAO,CAAC,CAAC,CAAC;MACtE,IAAI,CAACC,QAAO,GAAI,IAAI,CAAClB,IAAI,CAACmB,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC5D,IAAI,CAACC,YAAW,GAAI,IAAI,CAACrB,QAAQ,CAACkB,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MACpE,IAAI,CAACE,SAAQ,GAAI,IAAI,CAACrB,KAAK,CAACiB,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC9D,IAAI,CAACG,UAAS,GAAI,IAAI,CAACrB,MAAM,CAACgB,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAChE,IAAI,CAACI,SAAQ,GAAI,IAAI,CAACrB,KAAK,CAACe,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAE9D,IAAI,CAACK,mBAAkB,GAAK,uBAAsB,IAAI,CAACJ,YAAa,GAAE,IAAI,CAACrB,QAAS,EAAC;MACrF,IAAI,CAAC0B,qBAAoB,GAAK,GAAE,IAAI,CAACJ,SAAU,GAAER,UAAU,CAAC,IAAI,CAACb,KAAK,CAAC,CAACe,OAAO,CAAC,CAAC,CAAE,EAAC;MACpF,IAAI,CAACW,gBAAe,GAAK,KAAI,IAAI,CAACH,SAAU,GAAE,IAAI,CAACrB,KAAM,EAAC;MAE1D,IAAI,CAACc,QAAO,GAAK,KAAI,IAAI,CAACA,QAAS,GAAE,IAAI,CAAClB,IAAK,EAAC;MAChD,IAAI,CAACwB,UAAS,GAAK,KAAI,IAAI,CAACA,UAAW,GAAE,IAAI,CAACrB,MAAO,EAAC;MAEtD,IAAI,CAAC0B,eAAc,GAAK,KAAI,CAAC,EAAC,GAAI,IAAI,CAAC5B,QAAQ,CAACkB,MAAM,EAAEC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAE,GAAE,IAAI,CAACpB,QAAS,EAAC;MACrG,IAAI,CAAC6B,iBAAgB,GAAK,KAAI,CAAC,IAAIhB,UAAU,CAACK,MAAM,EAAEC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC